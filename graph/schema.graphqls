type Query {
  cars: [Car!]!
  car(id: ID!): Car
  persons: [Person!]!
  person(id: ID!): Person
}

type Mutation {
  addCar(input: CarInput!): Car!
  createPerson(input: PersonInput!): Person!
}

type Car {
  id: ID!
  model: String!
  owner: Person!
}

type Person {
  id: ID!
  name: String!
  address: String!

}

input CarInput {
  model: String!
  owner: ID!
}

input PersonInput {
  name: String!
  address: String!
}